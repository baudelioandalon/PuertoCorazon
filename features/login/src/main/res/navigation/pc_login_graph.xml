<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_login"
    app:startDestination="@id/ALoginFragment">

    <fragment
        android:id="@+id/ALoginFragment"
        android:name="com.boreal.puertocorazon.login.ui.ALoginFragment"
        android:label="ALoginFragment"
        tools:layout="@layout/a_login_fragment">
        <action
            android:id="@+id/action_ALoginFragment_to_pc_client_home_graph"
            app:destination="@id/pc_client_home_graph" />
        <action
            android:id="@+id/action_ALoginFragment_to_pc_adm_home_graph"
            app:destination="@id/pc_adm_home_graph" />
    </fragment>

    <include app:graph="@navigation/pc_client_home_graph" />
    <include app:graph="@navigation/pc_adm_home_graph" />


</navigation>
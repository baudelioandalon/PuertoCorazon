<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CuRecyclerView">
        <attr name="loader_listener">
            <flag name="none" value="0" />
            <flag name="loading" value="1" />
            <flag name="success" value="2" />
            <flag name="failure" value="3" />
        </attr>
        <attr name="recycler_orientation">
            <flag name="horizontal" value="0" />
            <flag name="vertical" value="1" />
        </attr>
    </declare-styleable>

</resources>